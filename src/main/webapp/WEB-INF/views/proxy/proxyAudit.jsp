<%@ page contentType="text/html;charset=UTF-8" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %><c:set var="ctx" value="${pageContext.request.contextPath}"/><!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9"> <![endif]-->
<!--[if !IE]><!--> <html lang="en"> <!--<![endif]-->
<!-- BEGIN HEAD -->
<head>
	<meta charset="utf-8" />
	<title>代理信息</title>

</head>
<!-- END HEAD -->
<!-- BEGIN BODY -->
<body class="page-header-fixed">
	<!-- BEGIN CONTAINER -->
	<div class="page-container row-fluid">
		<!-- BEGIN SIDEBAR -->
			<!-- Import Menu -->        			<jsp:include page="../../layouts/menu.jsp"></jsp:include>
		<!-- END SIDEBAR -->
		<!-- BEGIN PAGE -->
		<div class="page-content">
			<!-- BEGIN PAGE CONTAINER-->        
			<div class="container-fluid">
				<!-- BEGIN PAGE HEADER-->
				<div class="row-fluid">
					<div class="span12">
						<!-- BEGIN STYLE CUSTOMIZER -->						<!-- Import Style Setting Button -->        						<jsp:include page="../../layouts/style-setting.jsp"></jsp:include>
						<!-- END BEGIN STYLE CUSTOMIZER -->  
						<!-- BEGIN PAGE TITLE & BREADCRUMB-->						<h3 class="page-title">代理审核</h3>						<ul class="breadcrumb">							<li>								<i class="icon-home"></i>								<a href="${ctx}">首页</a> 								<i class="icon-angle-right"></i>							</li>							<li>代理管理<i class="icon-angle-right"></i></li>							<li>代理审核</li>													</ul>						<!-- END PAGE TITLE & BREADCRUMB-->
					</div>
				</div>
				<!-- END PAGE HEADER-->
				<!-- BEGIN PAGE CONTENT-->          
				<div class="row-fluid">					<div class="span12">						<!-- BEGIN VALIDATION STATES-->						<div class="portlet box purple">							<div class="portlet-title">								<div class="caption"><i class="icon-reorder"></i>代理审核</div>								<div class="tools">									<a href="javascript:;" class="collapse"></a>								</div>							</div>														<div class="portlet-body form">														<div class="form-horizontal form-view">															<!-- BEGIN FORM-->							<form action="${ctx}/proxy/audit/save" id="inputForm" method="post" class="form-horizontal">								<input type="hidden" name="id" value="${proxy.id}"/>								<h3 class="form-section">代理信息</h3>								<!-- BEGIN FORM-->								<div class="row-fluid">									<div class="span6">										<div class="control-group">											<label class="control-label">组织机构代码</label>											<div class="controls">												<span class="text">${proxy.orgcode}</span>											</div>										</div>									</div>									<div class="span6">										<div class="control-group">											<label class="control-label">中文名</label>											<div class="controls">												<span class="text">${proxy.nameCn}</span>											</div>										</div>									</div>								</div>								<div class="row-fluid">									<div class="span6">										<div class="control-group">											<label class="control-label">英文名</label>											<div class="controls">												<span class="text">${proxy.nameEn}</span>											</div>										</div>									</div>									<div class="span6">										<div class="control-group">											<label class="control-label">营业执照注册号</label>											<div class="controls">												<span class="text">${proxy.registeredNumber}</span>											</div>										</div>									</div>								</div>								<div class="row-fluid">									<div class="span6">										<div class="control-group">											<label class="control-label">税务登记证号</label>											<div class="controls">												<span class="text">${proxy.taxNumber}</span>											</div>										</div>									</div>									<div class="span6">										<div class="control-group">											<label class="control-label">所属行业</label>											<div class="controls">												<span class="text">${proxy.industry}</span>											</div>										</div>									</div>								</div>								<div class="row-fluid">									<div class="span6">										<div class="control-group">											<label class="control-label">单位性质</label>											<div class="controls">												<span class="text">${proxy.unitPropertie}</span>											</div>										</div>									</div>									<div class="span6">										<div class="control-group">											<label class="control-label">成立时间</label>											<div class="controls">												<span class="text">${proxy.foundedDate}</span>											</div>										</div>									</div>								</div>								<div class="row-fluid">									<div class="span6">										<div class="control-group">											<label class="control-label">经营范围</label>											<div class="controls">												<span class="text">${proxy.businessScope}</span>											</div>										</div>									</div>									<div class="span6">										<div class="control-group">											<label class="control-label">年营业额</label>											<div class="controls">												<span class="text">${proxy.annualTurnover}</span>											</div>										</div>									</div>								</div>								<div class="row-fluid">									<div class="span6">										<div class="control-group">											<label class="control-label">员工人数</label>											<div class="controls">												<span class="text">${proxy.employeeNumber}</span>											</div>										</div>									</div>									<div class="span6">										<div class="control-group">											<label class="control-label">法人代表</label>											<div class="controls">												<span class="text">${proxy.legalRepresentative}</span>											</div>										</div>									</div>								</div>								<div class="row-fluid">									<div class="span6">										<div class="control-group">											<label class="control-label">基本账户开户行</label>											<div class="controls">												<span class="text">${proxy.basicBank}</span>											</div>										</div>									</div>									<div class="span6">										<div class="control-group">											<label class="control-label">基本账户账号</label>											<div class="controls">												<span class="text">${proxy.basicBankAccount}</span>											</div>										</div>									</div>								</div>								<div class="row-fluid">									<div class="span6">										<div class="control-group">											<label class="control-label">注册资金</label>											<div class="controls">												<span class="text">${proxy.registeredCapital}</span>											</div>										</div>									</div>									<div class="span6">										<div class="control-group">											<label class="control-label">单位网址</label>											<div class="controls">												<span class="text">${proxy.website}</span>											</div>										</div>									</div>								</div>								<div class="row-fluid">									<div class="span6">										<div class="control-group">											<label class="control-label">注册地址</label>											<div class="controls">												<span class="text">${proxy.registeredAddress}</span>											</div>										</div>									</div>									<div class="span6">										<div class="control-group">											<label class="control-label">联系人地址</label>											<div class="controls">												<span class="text">${proxy.contactsAddress}</span>											</div>										</div>									</div>								</div>								<div class="row-fluid">									<div class="span6">										<div class="control-group">											<label class="control-label">邮编</label>											<div class="controls">												<span class="text">${proxy.zip}</span>											</div>										</div>									</div>									<div class="span6">										<div class="control-group">											<label class="control-label">联系人</label>											<div class="controls">												<span class="text">${proxy.contacts}</span>											</div>										</div>									</div>								</div>								<div class="row-fluid">									<div class="span6">										<div class="control-group">											<label class="control-label">联系电话</label>											<div class="controls">												<span class="text">${proxy.contactsPhone}</span>											</div>										</div>									</div>								</div>																<h3 class="form-section">审核</h3>								<div class="row-fluid">									<c:if test="${proxy.auditStatus==0 || proxy.auditStatus==2}">										<div class="span6">											<div class="control-group">												<label class="control-label"></label>												<div class="controls">													<div class="text-toggle-button">														<c:choose>															<c:when test="${proxy.auditStatus==2}">																<input name="auditStatus" value="1" type="checkbox" class="toggle" />															</c:when>															<c:otherwise>																<input name="auditStatus" value="1" type="checkbox" class="toggle" checked/>															</c:otherwise>														</c:choose>													</div>												</div>											</div>										</div>									</c:if>																	</div>								<c:if test="${proxy.auditStatus==1}">									<div class="alert alert-block alert-success ">										<button type="button" class="close" data-dismiss="alert"></button>										<h4 class="alert-heading">审核结果为：<span class="label label-success">通过</span></h4>									</div>								</c:if>								<c:if test="${proxy.auditStatus==2}">									<div class="alert alert-block alert-success ">										<button type="button" class="close" data-dismiss="alert"></button>										<h4 class="alert-heading">审核结果为：<span class="label label-important">不通过</span></h4>									</div>								</c:if>																<div class="form-actions">									<c:if test="${proxy.auditStatus==0 || proxy.auditStatus==2}">										<button type="submit" class="btn purple">提交</button>									</c:if>									<a href="${ctx}/proxy/audit/list" class="btn">返回</a>									<c:if test="${proxy.auditStatus==1}">									<a href="${ctx}/admin/proxy-admin/create" class="btn">新增代理管理员</a>									</c:if>								</div>																</form>								<!-- END FORM-->							</div>							</div>						</div>						<!-- END VALIDATION STATES-->					</div>				</div>
				<!-- END PAGE CONTENT-->
			</div>
			<!-- END PAGE CONTAINER-->
		</div>
		<!-- END PAGE -->
	</div>		<script>		$(document).ready(function() {			$('.text-toggle-button').toggleButtons({	            width: 200,	            height: 30,	            label: {	                enabled: "通过",	                disabled: "不通过"	            },	            style: {	                // Accepted values ["primary", "danger", "info", "success", "warning"] or nothing	                enabled: "success",	                disabled: "danger"	            }	        });		});	</script></body>
	
</html>