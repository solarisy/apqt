<%@ page contentType="text/html;charset=UTF-8" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %><c:set var="ctx" value="${pageContext.request.contextPath}"/><!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9"> <![endif]-->
<!--[if !IE]><!--> <html lang="en"> <!--<![endif]-->
<!-- BEGIN HEAD -->
<head>
	<meta charset="utf-8" />
	<title>代理信息</title>

</head>
<!-- END HEAD -->
<!-- BEGIN BODY -->
<body class="page-header-fixed">
	<!-- BEGIN CONTAINER -->
	<div class="page-container row-fluid">
		<!-- BEGIN SIDEBAR -->
			<!-- Import Menu -->        			<jsp:include page="../../layouts/menu.jsp"></jsp:include>
		<!-- END SIDEBAR -->
		<!-- BEGIN PAGE -->
		<div class="page-content">
			<!-- BEGIN PAGE CONTAINER-->        
			<div class="container-fluid">
				<!-- BEGIN PAGE HEADER-->
				<div class="row-fluid">
					<div class="span12">
						<!-- BEGIN STYLE CUSTOMIZER -->						<!-- Import Style Setting Button -->        						<jsp:include page="../../layouts/style-setting.jsp"></jsp:include>
						<!-- END BEGIN STYLE CUSTOMIZER -->  
						<!-- BEGIN PAGE TITLE & BREADCRUMB-->						<h3 class="page-title">代理信息</h3>						<ul class="breadcrumb">							<li>								<i class="icon-home"></i>								<a href="${ctx}">首页</a> 								<i class="icon-angle-right"></i>							</li>							<li>代理管理<i class="icon-angle-right"></i></li>							<li>代理信息</li>													</ul>						<!-- END PAGE TITLE & BREADCRUMB-->
					</div>
				</div>
				<!-- END PAGE HEADER-->
				<!-- BEGIN PAGE CONTENT-->          
				<div class="row-fluid">					<div class="span12">						<!-- BEGIN VALIDATION STATES-->						<div class="portlet box purple">							<div class="portlet-title">								<div class="caption"><i class="icon-reorder"></i>代理信息</div>								<div class="tools">									<a href="javascript:;" class="collapse"></a>								</div>							</div>																					<div class="portlet-body form">							<div class="form-horizontal form-view">								<div id="message" class="alert alert-success">代理创建成功，已提交审核。</div>								<!-- BEGIN FORM-->								<div class="row-fluid">									<div class="span6">										<div class="control-group">											<label class="control-label">组织机构代码</label>											<div class="controls">												<span class="text">${proxy.orgcode}</span>											</div>										</div>									</div>									<div class="span6">										<div class="control-group">											<label class="control-label">中文名</label>											<div class="controls">												<span class="text">${proxy.nameCn}</span>											</div>										</div>									</div>								</div>								<div class="row-fluid">									<div class="span6">										<div class="control-group">											<label class="control-label">英文名</label>											<div class="controls">												<span class="text">${proxy.nameEn}</span>											</div>										</div>									</div>									<div class="span6">										<div class="control-group">											<label class="control-label">营业执照注册号</label>											<div class="controls">												<span class="text">${proxy.registeredNumber}</span>											</div>										</div>									</div>								</div>								<div class="row-fluid">									<div class="span6">										<div class="control-group">											<label class="control-label">税务登记证号</label>											<div class="controls">												<span class="text">${proxy.taxNumber}</span>											</div>										</div>									</div>									<div class="span6">										<div class="control-group">											<label class="control-label">所属行业<span class="required">*</span></label>											<div class="controls">												<span class="text">${proxy.industry}</span>											</div>										</div>									</div>								</div>								<div class="row-fluid">									<div class="span6">										<div class="control-group">											<label class="control-label">单位性质</label>											<div class="controls">												<span class="text">${proxy.unitPropertie}</span>											</div>										</div>									</div>									<div class="span6">										<div class="control-group">											<label class="control-label">成立时间</label>											<div class="controls">												<span class="text">${proxy.foundedDate}</span>											</div>										</div>									</div>								</div>								<div class="row-fluid">									<div class="span6">										<div class="control-group">											<label class="control-label">经营范围</label>											<div class="controls">												<span class="text">${proxy.businessScope}</span>											</div>										</div>									</div>									<div class="span6">										<div class="control-group">											<label class="control-label">年营业额</label>											<div class="controls">												<span class="text">${proxy.annualTurnover}</span>											</div>										</div>									</div>								</div>								<div class="row-fluid">									<div class="span6">										<div class="control-group">											<label class="control-label">员工人数</label>											<div class="controls">												<span class="text">${proxy.employeeNumber}</span>											</div>										</div>									</div>									<div class="span6">										<div class="control-group">											<label class="control-label">法人代表</label>											<div class="controls">												<span class="text">${proxy.legalRepresentative}</span>											</div>										</div>									</div>								</div>								<div class="row-fluid">									<div class="span6">										<div class="control-group">											<label class="control-label">基本账户开户行</label>											<div class="controls">												<span class="text">${proxy.basicBank}</span>											</div>										</div>									</div>									<div class="span6">										<div class="control-group">											<label class="control-label">基本账户账号</label>											<div class="controls">												<span class="text">${proxy.basicBankAccount}</span>											</div>										</div>									</div>								</div>								<div class="row-fluid">									<div class="span6">										<div class="control-group">											<label class="control-label">注册资金</label>											<div class="controls">												<span class="text">${proxy.registeredCapital}</span>											</div>										</div>									</div>									<div class="span6">										<div class="control-group">											<label class="control-label">单位网址</label>											<div class="controls">												<span class="text">${proxy.website}</span>											</div>										</div>									</div>								</div>								<div class="row-fluid">									<div class="span6">										<div class="control-group">											<label class="control-label">注册地址</label>											<div class="controls">												<span class="text">${proxy.registeredAddress}</span>											</div>										</div>									</div>									<div class="span6">										<div class="control-group">											<label class="control-label">联系人地址</label>											<div class="controls">												<span class="text">${proxy.contactsAddress}</span>											</div>										</div>									</div>								</div>								<div class="row-fluid">									<div class="span6">										<div class="control-group">											<label class="control-label">邮编</label>											<div class="controls">												<span class="text">${proxy.zip}</span>											</div>										</div>									</div>									<div class="span6">										<div class="control-group">											<label class="control-label">联系人</label>											<div class="controls">												<span class="text">${proxy.contacts}</span>											</div>										</div>									</div>								</div>								<div class="row-fluid">									<div class="span6">										<div class="control-group">											<label class="control-label">联系电话</label>											<div class="controls">												<span class="text">${proxy.contactsPhone}</span>											</div>										</div>									</div>								</div>								<div class="form-actions">									<a href="${ctx}/proxy/create" class="btn">继续添加</a>								</div>								<!-- END FORM-->							</div>							</div>						</div>						<!-- END VALIDATION STATES-->					</div>				</div>
				<!-- END PAGE CONTENT-->
			</div>
			<!-- END PAGE CONTAINER-->
		</div>
		<!-- END PAGE -->
	</div>	</body>
	
</html>